class Vertex{
    public int add ( int a , int b){
        return a+b;
    }
    public int add (int a, int b, int c){
        return a+b+c;    
    }
    public int add(int a , int b, int c, int d){
        return a+b+c+d;
    }
}

public class demo{
    public static void main(String[] args) {
       
    
        Vertex obj =new Vertex();
         int result = obj.add(1,2);
        System.out.println(result);
    }
}

